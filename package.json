{
  "name": "real-world",
  "version": "0.0.1",
  "description": "An API for publishing and reading articles.",
  "author": "Vin√≠cius Vargas",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start:prod": "node dist/main/server",
    "start": "NODE_ENV=development nest start",
    "start:dev": "NODE_ENV=development nest start",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write .",
    "test:unit": "jest --testRegex .*\\.unit\\.ts$",
    "test:acceptance": "jest --testRegex .*\\.acceptance\\.ts$",
    "openapi": "ts-node main/nest/openapi",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.12",
    "@nestjs/core": "^9.3.12",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/swagger": "^6.3.0",
    "aws-lambda": "^1.0.7",
    "axios": "^1.3.4",
    "aws-serverless-express": "^3.4.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "jsonwebtoken": "^9.0.0",
    "mysql": "^2.18.1",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "rxjs": "^7.8.0",
    "typeorm": "0.3.12",
    "typeorm-naming-strategies": "^4.1.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.1.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/aws-serverless-express": "^3.3.5",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "@types/passport-http": "^0.3.9",
    "@types/passport-jwt": "^3.0.8",
    "@vercel/ncc": "^0.36.1",
    "jest": "^29.5.0",
    "lorem-ipsum": "^2.0.8",
    "prettier": "^2.8.7",
    "prettier-plugin-organize-imports": "^3.2.2",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.6.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testRegex": ".*\\.(unit|spec|acceptance)\\.ts$",
    "collectCoverageFrom": [
      "main/*.(t|j)s"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage"
  }
}
